{
	"name": "terminal-setups",
	"displayName": "Terminal Setups",
	"description": "Open multiple terminals from a configuration",
	"publisher": "linderjohan",
	"author": {
		"name": "Johan Linder",
		"email": "35808649+linderjohan@users.noreply.github.com"
	},
	"version": "0.0.3",
